  vertical-align:middle;
	background: {{settings.button_color}}; 
	background: -moz-linear-gradient(top, {{ settings.button_color_top }} 0%, {{settings.button_color}} 100%); 
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color_top }}), color-stop(100%,{{settings.button_color}})); 
	padding:4px 8px 3px; color:#ffffff; font-size:11px; font-weight:bold; text-shadow:1px 1px  #777;}
	
#header .vertical-navs ul.cart .checkout a:hover { 
background: {{settings.button_color_top }};background: -moz-linear-gradient(top, {{ settings.button_color }} 0%, {{settings.button_color_top}} 100%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color }}), color-stop(100%,{{settings.button_color_top}})); text-decoration:none;}

#header .vertical-navs ul.cart .checkout a:hover { cursor:pointer; }	

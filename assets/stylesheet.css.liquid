.extra-border h2 { border-bottom:4px solid {{ settings.active_border }}; padding-bottom:2px; margin-bottom:-4px; float:left; position:relative;}
.extra-border {  border-bottom:4px solid {{ settings.light_border }}; clear:both; margin-bottom:30px; }

/* cart */

.cart .extra-border h1 { border-bottom:4px solid {{ settings.active_border }}; padding-bottom:3px; margin-bottom:-4px; float:left; position:relative;}
.cart .extra-border { border-bottom:4px solid {{ settings.light_border }}; clear:both;margin-bottom:30px }

#cart table { border-collapse:collapse; margin-bottom:25px; width:100%; }
#cart th { border-bottom:1px solid {{ settings.light_border }}; border-top:1px solid {{ settings.light_border }}; padding-bottom:15px; padding-top:15px; text-align:left; } 
#cart td { border-bottom:1px solid {{ settings.light_border }}; padding:20px 0; }
#cart .cart-options { float:right;  } 
#cart .cart-options h2 { font-size:20px; font-weight:normal; letter-spacing:-1px; text-align:right; } 
#cart .cart-options h2 strong { vertical-align:top; }
.cart-product-details a { } .cart-product-details a img { border:1px solid {{ settings.light_border }}; margin-right:10px; } .cart-product-details a * { vertical-align:middle; } 
.cart-options #update-cart { background:transparent; color:{{settings.link_color }}; cursor:pointer; font-size:15px; font-weight:bold; margin-right:2px; text-shadow:none; border:none; padding:0;} 
.cart-options #update-cart .standard-button { margin-left:5px}
.cart-options #update-cart:hover { border:medium none; color: {{ settings.link_hover_color }}; } .cart-options p { line-height:40px; vertical-align:middle; } 
.cart-options .standard-button {margin-left:6px}

/* paginate */
#paginate { text-align:center;}
#paginate .deco { margin:0 6px}


#paginate span { display:inline-block; white-space: nowrap;}

#paginate .next a, #paginate  .prev a { font-size:11px; text-transform:uppercase; color:{{ settings.active_border }}; margin-right:10px; padding:5px; font-weight:bold; display:none;}
#paginate .next a { margin-left:0; margin-left:10px}
#paginate .next a, #paginate  .prev a { text-decoration:none}

#paginate .page a, #paginate  .page.current { margin:0 0 0 -4px;font-size:17px;    padding:6px 10px; border-bottom:4px solid {{ settings.light_border }}; color:{{ settings.light_border }}; font-weight:bold; }
#paginate .page.current {  border-bottom:4px solid {{ settings.active_border }}; font-weight:bold; color:{{ settings.active_border }} }
#paginate .page a:hover { color:{{ settings.active_border }}; border-color:{{ settings.active_border }}; text-decoration:none}

#quick-cart { position:absolute; top:0; background:#000; background:rgba(0,0,0,0.9); width:100%; z-index:5;}
